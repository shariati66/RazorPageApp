@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}

<div class="text-center">
    <table class="table table-striped table-bordered">
    <tr>
        <th>First Name</th>
        <th>Last Name</th>
    </tr>
    
    @foreach(var obj in Model.operators)
    {
        <tr>
            <td>@obj.FirstName</td>
            <td>@obj.LastName</td>
        </tr>
    }
</table>
<a class="btn btn-primary" asp-page="Create" >Create User</a>
</div>
